{
  "name": "tailwind-config-ruby",
  "version": "0.0.1",
  "description": "Node package for tailwind_config Ruby gem",
  "main": "pkg/dist-node/index.js",
  "scripts": {
    "build": "pika-pack build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewmcodes/tailwind_config_gem.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "ruby",
    "gem",
    "rails"
  ],
  "author": "Andrew Mason",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewmcodes/tailwind_config_gem/issues"
  },
  "homepage": "https://github.com/andrewmcodes/tailwind_config_gem#readme",
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-simple-bin",
        {
          "bin": "tailwind-config-ruby"
        }
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-simple-bin": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@pika/types": "^0.9.2",
    "@types/node": "^10.12.18",
    "prettier": "^2.1.1",
    "tailwindcss": "^1.7.6",
    "typescript": "^3.7.0"
  }
}
